# redirect all requests to handle proxy
server {
    server_name handle.datacite.org;
    listen 80;
    resolver <%= @resolver %>;

    location / {
        return 301 https://handle.datacite.org$request_uri;
    }
}

server {
    server_name handle.datacite.org;
    listen 8080;
    resolver <%= @resolver %>;

    location / {
        return 301 https://<%= @backend %>;
    }
}
